// Exercise 2 - FizzBuzz

for (let count = 0; count<101; count+=1){
  if(count==0){console.log(0)}
  else if ((count%3==0) && (count%5==0)){
      console.log('FizzBuzz')}
  else if (count%5==0){
    console.log('Buzz')
  }
  else if (count%3==0){
   console.log('Fizz')
  }
  else {
    console.log(count)
  };
}
